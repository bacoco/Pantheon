{
  "name": "Pantheon Lightning",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0a0a",
    "editor.foreground": "#e0e0e0",
    "activityBar.background": "#1a1a2e",
    "activityBar.foreground": "#ffd700",
    "activityBar.activeBorder": "#ffd700",
    "activityBarBadge.background": "#ff6b6b",
    "sideBar.background": "#0f0f1e",
    "sideBar.foreground": "#c0c0c0",
    "sideBarTitle.foreground": "#ffd700",
    "statusBar.background": "#1a1a2e",
    "statusBar.foreground": "#ffd700",
    "statusBar.noFolderBackground": "#1a1a2e",
    "statusBarItem.hoverBackground": "#2a2a3e",
    "titleBar.activeBackground": "#1a1a2e",
    "titleBar.activeForeground": "#ffd700",
    "titleBar.inactiveBackground": "#0f0f1e",
    "terminal.background": "#0a0a0a",
    "terminal.foreground": "#e0e0e0",
    "terminal.ansiYellow": "#ffd700",
    "terminal.ansiBlue": "#4fc3f7",
    "terminal.ansiGreen": "#66bb6a",
    "terminal.ansiRed": "#ff6b6b",
    "tab.activeBackground": "#1a1a2e",
    "tab.activeForeground": "#ffd700",
    "tab.inactiveBackground": "#0f0f1e",
    "editorGroupHeader.tabsBackground": "#0f0f1e",
    "button.background": "#ffd700",
    "button.foreground": "#000000",
    "button.hoverBackground": "#ffed4e",
    "input.background": "#1a1a2e",
    "input.border": "#ffd700",
    "focusBorder": "#ffd700",
    "selection.background": "#ffd70040",
    "progressBar.background": "#ffd700",
    "scrollbarSlider.background": "#ffd70020",
    "scrollbarSlider.hoverBackground": "#ffd70040",
    "scrollbarSlider.activeBackground": "#ffd70060"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a6a6a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#66bb6a"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#4fc3f7"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#e0e0e0"
      }
    }
  ]
}